<?xml version="1.0" encoding="utf-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Crow"
    x:Class="Crow.AppShell">

    <ShellContent Route="MainPage"
            ContentTemplate="{DataTemplate local:MainPage}"/>
    <ShellContent Route="HomePage"
            ContentTemplate="{DataTemplate local:HomePage}"/>
    <ShellContent Route="ContasAVerPage"
            ContentTemplate="{DataTemplate local:ContasAVerPage}"/>
    <ShellContent Route="ContasAReceberPage"
            ContentTemplate="{DataTemplate local:ContasAReceberPage}"/>
    <ShellContent Route="EntradaPage"
            ContentTemplate="{DataTemplate local:EntradaPage}"/>
    <ShellContent Route="SaidaPage"
            ContentTemplate="{DataTemplate local:SaidaPage}"/>

</Shell>

